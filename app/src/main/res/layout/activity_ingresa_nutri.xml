<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".IngresaNutri">

    <TextView
        android:id="@+id/txtCitasAgen"
        android:layout_width="318dp"
        android:layout_height="59dp"
        android:textStyle="bold"
        android:gravity="center"
        android:textSize="25dp"
        android:text="Citas Agendadas"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="29dp" />

    <TextView
        android:id="@+id/txtFecha"
        android:layout_width="97dp"
        android:layout_height="46dp"
        android:textSize="20dp"
        android:textStyle="bold"
        android:text="Fecha"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="88dp" />

    <EditText
        android:id="@+id/editTextDate"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="208dp" />

    <EditText
        android:id="@+id/edtFecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="134dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="283dp"
        android:layout_height="218dp"
        app:srcCompat="@drawable/nutriologo_en_linea_640x400"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="283dp" />

    <Button
        android:id="@+id/btnRevisa"
        android:layout_width="131dp"
        android:layout_height="47dp"
        android:text="Revisar"
        tools:layout_editor_absoluteX="140dp"
        tools:layout_editor_absoluteY="508dp" />
</android.support.constraint.ConstraintLayout>